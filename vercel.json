{
  "version": 2,
  "functions": {
    "api/checkout/pagamento/index.js": {
      "runtime": "nodejs20.x"
    },
    "api/checkout/verificar/index.js": {
      "runtime": "nodejs20.x"
    },
    "api/fb-conversions-api/index.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    { "src": "/api/checkout/pagamento", "dest": "api/checkout/pagamento/index.js" },
    { "src": "/api/checkout/verificar", "dest": "api/checkout/verificar/index.js" },
    { "src": "/api/fb-conversions-api", "dest": "api/fb-conversions-api/index.js" },
    { "src": "/api/payment", "dest": "/api/payment.js" },
    { "src": "/api/verify", "dest": "/api/verify.js" }
  ],
  "rewrites": [
    { "source": "/api/checkout/pagamento", "destination": "/api/checkout/pagamento/index.js" },
    { "source": "/api/checkout/verificar", "destination": "/api/checkout/verificar/index.js" },
    { "source": "/api/fb-conversions-api", "destination": "/api/fb-conversions-api/index.js" },
    { "source": "/api/payment", "destination": "/api/payment.js" },
    { "source": "/api/verify", "destination": "/api/verify.js" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "X-Requested-With, Content-Type, Accept, Authorization" }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "X-Requested-With, Content-Type, Accept, Authorization" }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_VERSION": "20.x"
    }
  }
} 